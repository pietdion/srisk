	Eprint = {http://www.demographic-research.org/volumes/vol31/2/31-2.pdf},
	Journal = {Demographic Research},
	Number = {2},
	Pages = {27-70},
	Title = {{Factors responsible for mortality variation in the United States: A latent variable analysis}},
